# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'Shimmer-POC' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
    pod 'Fabric', '~> 1.9.0'
    pod 'Alamofire'
    pod 'MKProgress', '~> 1.0.8' 	
    pod 'iOSDropDown'	# Dropdown ist pod

  # Pods for Shimmer-POC

  target 'Shimmer-POCTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Shimmer-POCUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
